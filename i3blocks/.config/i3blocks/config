command=~/.config/i3blocks/$BLOCK_NAME
separator_block_width=5
markup=none


[cmus_status]
align=center
interval=1
markup=pango

[keyboard]
command=~/.config/i3blocks/xkblayout-state print "%s" | tr '[:lower:]' '[:upper:]'
interval=1
min_width=" MM "
align=center

[volume]
# label= 奔
instance=Master
min_width=" 婢100 "
align=left
interval=1
markup=pango

[time]
command=date '+  %d %b  %H:%M:%S   ' | tr '[:lower:]' '[:upper:]'
interval=1

